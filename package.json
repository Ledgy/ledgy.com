{
  "name": "ledgy-landing-page",
  "description": "Ledgy Landing Page",
  "version": "1.0.0",
  "author": "Ledgy AG <contact@ledgy.com>",
  "repository": "morloy/ledgy-app",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "lingui compile && gatsby build",
    "start": "lingui compile && gatsby develop",
    "test": "lingui compile && eslint . --ext jsx,js && flow",
    "extract": "lingui extract --clean"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@lingui/react": "2.7.0",
    "aos": "2.3.3",
    "babel-core": "7.0.0-bridge.0",
    "bootstrap": "4.1.3",
    "et-line": "1.0.1",
    "font-awesome": "4.7.0",
    "gatsby": "2.0.14",
    "gatsby-image": "2.0.13",
    "gatsby-plugin-flow": "1.0.2",
    "gatsby-plugin-google-tagmanager": "2.0.5",
    "gatsby-plugin-layout": "1.0.3",
    "gatsby-plugin-manifest": "2.0.4",
    "gatsby-plugin-netlify": "2.0.0",
    "gatsby-plugin-netlify-cache": "1.0.0",
    "gatsby-plugin-nprogress": "2.0.5",
    "gatsby-plugin-offline": "2.0.5",
    "gatsby-plugin-react-helmet": "3.0.0",
    "gatsby-plugin-sass": "2.0.1",
    "gatsby-plugin-sharp": "2.0.5",
    "gatsby-plugin-sitemap": "2.0.1",
    "gatsby-plugin-sri": "1.0.3",
    "gatsby-source-filesystem": "2.0.1",
    "gatsby-transformer-sharp": "2.1.3",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.3.1",
    "lity": "2.3.1",
    "lodash": "4.17.11",
    "node-sass": "4.9.3",
    "popper.js": "1.14.4",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-helmet": "5.2.0",
    "slick-carousel": "1.8.1",
    "typeface-slabo-27px": "0.0.54",
    "typeface-work-sans": "0.0.54"
  },
  "keywords": [
    "ledgy"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:flowtype/recommended",
      "airbnb"
    ],
    "plugins": [
      "flowtype"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "object-curly-newline": [
        "error",
        {
          "multiline": true
        }
      ]
    }
  },
  "devDependencies": {
    "@lingui/babel-preset-react": "2.7.0",
    "@lingui/cli": "2.7.0",
    "babel-eslint": "9.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.82.0"
  },
  "lingui": {
    "fallbackLocale": "en",
    "sourceLocale": "en",
    "localeDir": "src/locale",
    "srcPathDirs": [
      "src/pages",
      "src/components",
      "src/layouts"
    ],
    "format": "po",
    "extractBabelOptions": {
      "presets": [
        "@babel/react",
        "@babel/flow"
      ]
    }
  }
}
